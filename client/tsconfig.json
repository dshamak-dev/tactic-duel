{
  "compilerOptions": {
    "baseUrl": "../",
    "rootDirs": ["client/src", "core"],
    "paths": {
      "client/*": ["client/src/*"],
      "src/*": [
         "client/src/*"
      ]
    },
    "outDir": "./dist/",
    "noImplicitAny": false,
    "module": "esnext",
    "target": "es5",
    "jsx": "react",
    "allowJs": true,
    "moduleResolution": "node",
    "allowSyntheticDefaultImports": true,
    "sourceMap": true
  }
}
