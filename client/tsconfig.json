{
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "client/*": ["./client/src/*"],
      "core/*": ["../core/*"],
      "src/*": [
         "src/*"
      ]
    },
    "outDir": "./dist/",
    "noImplicitAny": false,
    "module": "esnext",
    "target": "es5",
    "jsx": "react",
    "allowJs": true,
    "moduleResolution": "node",
    "allowSyntheticDefaultImports": true,
    "sourceMap": true
  }
}
